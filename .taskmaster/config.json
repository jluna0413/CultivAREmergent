{
  "project": {
    "name": "CultivAR",
    "description": "Cannabis Grow Journal Application",
    "version": "1.0.0",
    "root": ".",
    "workflowIntegration": {
      "enabled": true,
      "autoTaskCreation": true,
      "contextAwareSuggestions": true,
      "codeReviewIntegration": true,
      "documentationSync": true
    }
  },
  "ai": {
    "provider": "ollama",
    "model": "gemma3:270m",
    "baseUrl": "http://localhost:11435",
    "maxTokens": 4096,
    "temperature": 0.7
  },
  "features": {
    "parsePrd": true,
    "taskGeneration": true,
    "codeAnalysis": true,
    "documentation": true,
    "workflowEmbedding": true,
    "contextAwareness": true,
    "collaborativeFeatures": true
  },
  "workflow": {
    "taskIntegration": {
      "autoCreateTasks": true,
      "linkPRs": true,
      "syncDocumentation": true
    },
    "collaboration": {
      "enableComments": true,
      "shareInsights": true,
      "teamSync": true
    },
    "automation": {
      "codeReview": true,
      "testGeneration": true,
      "deploymentChecks": true
    }
  },
  "output": {
    "format": "json",
    "includeMetadata": true,
    "saveHistory": true,
    "workflowTracking": true
  }
}